{{#if hasBlockParams}}
  {{#ef-list ordered=ordered}}
    {{yield (hash
      item=(component
        "ef-list-item"
        active=active
        disabled=disabled
        isNav=true
      )
    )}}
  {{/ef-list}}
{{else}}
  You must use block params (<code>as | |</code>) when using the <code>ef-nav</code> component.
{{/if}}
